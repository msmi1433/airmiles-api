{
  "main": "listen.js",
  "name": "airmiles-seed",
  "description": "This project seeds a PSQL DB with data regarding the amount of air miles/points required to travel to destinations from London.",
  "version": "1.0.0",
  "scripts": {
    "setup-dbs": "psql -f ./db/setup.sql",
